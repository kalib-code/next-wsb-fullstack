datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
}


model User {
  id            String    @id @default(cuid())
  name          String?
  email         String   @unique
  hash          String?
  salt          String?
  image         String?
  createAt      DateTime  @default(now())
 
}


